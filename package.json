    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "libphonenumber-js": "^1.12.23",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "mockconsole": "0.0.1",
    "panzoom": "^9.4.3",
