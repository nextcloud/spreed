    "@nextcloud/router": "^3.0.1",
    "@nextcloud/sharing": "^0.3.0",
    "@nextcloud/upload": "^2.0.0-rc.0",
    "@nextcloud/vue": "^9.0.0",
    "@playwright/test": "^1.56.0",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
